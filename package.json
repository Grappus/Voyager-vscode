{
    "name": "package-manager",
    "displayName": "packageManager",
    "description": "Manage package.json with GUI in visual studio code",
    "version": "0.0.1",
    "publisher": "anish",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.managePackage"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.managePackage",
            "title": "Manage Packages"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "dev": "cd react && npm start",
        "build": "cd react && npm run build"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}